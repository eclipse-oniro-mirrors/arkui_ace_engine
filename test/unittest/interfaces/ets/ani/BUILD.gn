import("//foundation/arkui/ace_engine/test/unittest/ace_unittest.gni")

ohos_unittest("performance_monitor_test") {
  module_out_path = interface_test_output_path

  deps = [ "$ace_root/build:libace_compatible" ]

  include_dirs = [
    "$ace_root/interfaces/ets/ani/performanceMonitor/src/",
  ]

  sources = [
    #"performance_monitor_test.cpp",
  ]

  external_deps = [
    "googletest:gmock",
    "googletest:gtest_main",
    "runtime_core:ani",
  ]
  configs = [ "$ace_root:ace_config" ]
}

group("ace_ani_unittest") {
  testonly = true
  deps = [
    ":performance_monitor_test",
  ]
}
