import("//foundation/arkui/ace_engine/test/unittest/ace_unittest.gni")
import("../../capi_common.gni")
import("modifiers.gni")

ace_unittest("capi_generated_modifiers_test") {
  type = "new"
  module_output = "capi"
  sources = [
    "qr_code_modifier_test.cpp",
    "test_fixtures.cpp",
    "test_fixtures_enums.cpp",
  ]
  sources += generated_sources
  extra_deps = []
  extra_deps += [
    "${ace_root}/frameworks/core/interfaces/native:ace_core_interfaces_native_node_linux_unittest",
    "..:capi_deps",
    "../../stubs:capi-ut-stubs",
  ]
}

group("capi_generated_modifiers_unittest") {
  testonly = true
  deps = [ ":capi_generated_modifiers_test" ]
}
