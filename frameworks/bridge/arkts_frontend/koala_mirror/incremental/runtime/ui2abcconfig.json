{
    "compilerOptions": {
        "package": "@koalaui/runtime",
        "outDir": "build/recheck",
        "baseUrl": "./ets",
        "paths": {
            "@koalaui/common": ["../../common/src"],
            "@koalaui/compat": ["../../compat/src/arkts"],
            "@koalaui/runtime/annotations": ["../annotations"]
        },
        "plugins": [
           {
                "transform": "@koalaui/memo-plugin",
                "state": "parsed",
                "contextImport": "../internals",
                "name": "memo"
            },
            {
                "transform": "@koalaui/memo-plugin",
                "state": "checked",
                "name": "memo"
            }
        ]
    },
    "include": ["ets/**/*.ts"]
}
