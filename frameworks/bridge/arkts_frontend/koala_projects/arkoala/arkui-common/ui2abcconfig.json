{
    "compilerOptions": {
        "package": "@koalaui/arkui-common",
        "outDir": "./build-m3",
        "baseUrl": "./ets",
        "paths": {
            "@koalaui/compat": [
                "../../../incremental/compat/src/arkts"
            ],
            "@koalaui/common": [
                "../../../incremental/common/src"
            ],
            "@koalaui/runtime": [
                "../../../incremental/runtime/ets"
            ],
            "@koalaui/runtime/annotations": [
                "../../../incremental/runtime/annotations"
            ],
            "#platform": [
                "../../../incremental/compat/src/arkts"
            ]
        },
        "plugins": [
            {
                "transform": "@koalaui/memo-plugin",
                "stage": "checked",
                "name": "memo"
            }
        ]
    },
    "include": [
        "ets/index.ts",
        "ets/ArkState.ts",
        "ets/arkts/**/*.ts"
    ],
    "dependencies": {
        "@koalaui/runtime": [
            "../../incremental/runtime/ui2abcconfig.json"
        ]
    }
}
