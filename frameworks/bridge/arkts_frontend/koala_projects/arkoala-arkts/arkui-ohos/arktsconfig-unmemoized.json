{
    "compilerOptions": {
        "package": "arkui",
        "outDir": "build/abc",
        "baseUrl": "./build/unmemoized/src",
        "paths": {
            "#components": [
                "./component/arkts"
            ],
            "#arkcompat/*": [
                "../../../../../arkoala/arkui-common/build/unmemoized/src/arkts/*"
            ],
            "#external": [
                "../../../src/external/arkts"
            ],
            "arkui.ani": [
                "../../../src/ani/arkts"
            ],
            "#arkstate": [
                "../../../../../arkoala/arkui-common/build/unmemoized/src"
            ],
            "@koalaui/arkui-common": [
                "../../../../../arkoala/arkui-common/build/unmemoized/src"
            ],
            "@ohos/arkui": [
                "./"
            ],
            "@ohos/arkui/*": [
                "./*"
            ],
            "@koalaui/interop": [
                "../../../../../interop/src/arkts"
            ],
            "@koalaui/common": [
                "../../../../../incremental/common/src"
            ],
            "@koalaui/compat": [
                "../../../../../incremental/compat/src/arkts"
            ],
            "@koalaui/runtime": [
                "../../../../../incremental/runtime/build/unmemoized/src"
            ],
            "#helpindexarkui": [
                "index.ts"
            ],
            "@ohos/animator": [
                "../ohos.animator.ts"
            ],
            "@ohos/arkui/UIContext": [
                "../ohos.arkui.UIContext.ts"
            ],
            "@ohos/measure": [
                "../ohos.measure.ts"
            ],
            "@ohos/font/font": [
                "../ohos.font.ts"
            ],
            "global/resource": [
                "../global/resource/resource.ts"
            ]
        }
    },
    "include": [
        "build/unmemoized/src/**/*.ts",
        "build/unmemoized/src/*.ts",
        "build/unmemoized/ohos.arkui.UIContext.ts",
        "build/unmemoized/ohos.measure.ts",
        "build/unmemoized/ohos.font.ts",
        "build/unmemoized/ohos.animator.ts",
        "src/ani/**/*.ts"
    ],
    "exclude": [
        "**/index.ts",
        "build/unmemoized/src/component/ts/*.ts",
        "build/unmemoized/src/component/main.ts",
        "build/unmemoized/src/external/ts/**/*.ts",
        "build/unmemoized/src/ani/ts/**/*.ts"
    ]
}